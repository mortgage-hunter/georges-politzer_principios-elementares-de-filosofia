		<dc:title id="title">Princípios Elementares de Filosofia</dc:title>
		<meta property="file-as" refines="#title">Princípios Elementares de Filosofia</meta>
		<dc:subject id="subject-1">Philosophy</dc:subject>
		<dc:subject id="subject-2">Communism</dc:subject>
		<dc:subject id="subject-3">Socialism</dc:subject>
